{
  "functions": {
    "api/index.rs": {
      "runtime": "vercel-rust@3.0.0"
    }
  },
  "headers": [
    {
      "source": "/",
      "headers": [
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [{ "source": "/", "destination": "/api/index" }]
}
